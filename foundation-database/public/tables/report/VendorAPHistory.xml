 <querysource>
  <name>head</name>
  <sql>SELECT formatDate(&lt;? value('startDate') ?>, 'Earliest') AS startdate,
       formatDate(&lt;? value('endDate')   ?>, 'Latest')   AS enddate
&lt;? if exists('vend_id') ?>
     , vend_number,
       vend_name
  FROM vendinfo
 WHERE vend_id= &lt;? value('vend_id') ?>
&lt;? elseif exists('vendtype_id') ?>
     , vendtype_code AS vend_number,
      'Vendor Type'  AS vend_name
  FROM vendtype
 WHERE vendtype_id = &lt;? value('vendtype_id') ?>
&lt;? elseif exists('vendtype_pattern') ?>
     , &lt;? value('vendtype_pattern') ?> AS vend_number,
       'Vendor Types'                  AS vend_name
&lt;? endif ?>
;</sql>
 </querysource>
